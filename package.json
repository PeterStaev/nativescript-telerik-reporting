{
  "name": "nativescript-telerik-reporting",
  "version": "1.0.0",
  "description": "A helper library for accessing Telerik Reporting REST API",
  "main": "telerik-reporting",
  "typings": "telerik-reporting.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "3.0.0",
      "android": "3.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Utilities"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-telerik-reporting"
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json",
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android"
  },
  "keywords": [
    "NativeScript",
    "reporting",
    "telerik",
    "progress",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "license": "Apache-2.0",
  "peerDependencies": {
    "tns-core-modules": "^3.1.0 || >3.3.0-"
  },
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "tns-core-modules": "^3.4.0",
    "tns-platform-declarations": "^3.4.0",
    "tslint": "^4.5.1",
    "typescript": "^2.6.2"
  }
}
